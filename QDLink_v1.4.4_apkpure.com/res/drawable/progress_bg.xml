<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5.0dip" />
            <gradient android:startColor="@color/radio_white_gray" android:endColor="@color/radio_white_gray" android:angle="270.0" android:centerY="0.75" android:centerColor="@color/radio_white_gray" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="5.0dip" />
                <gradient android:startColor="@color/unConnectedBaseBlue" android:endColor="@color/unConnectedBaseBlue" android:angle="270.0" android:centerY="0.75" android:centerColor="@color/unConnectedBaseBlue" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5.0dip" />
                <gradient android:startColor="@color/unConnectedBaseBlue" android:endColor="@color/unConnectedBaseBlue" android:angle="270.0" android:centerY="0.75" android:centerColor="@color/unConnectedBaseBlue" />
            </shape>
        </clip>
    </item>
</layer-list>